{
    "java.home": "/Library/Java/JavaVirtualMachines/jdk1.8.0_181.jdk/Contents/Home",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "spellright.language": [
        "English"
    ],
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#2979FF",
        "list.activeSelectionForeground": "#2979FF",
        "list.inactiveSelectionForeground": "#2979FF",
        "list.highlightForeground": "#2979FF",
        "scrollbarSlider.activeBackground": "#2979FF50",
        "editorSuggestWidget.highlightForeground": "#2979FF",
        "textLink.foreground": "#2979FF",
        "progressBar.background": "#2979FF",
        "pickerGroup.foreground": "#2979FF",
        "tab.activeBorder": "#2979FF",
        "notificationLink.foreground": "#2979FF",
        "editorWidget.resizeBorder": "#2979FF",
        "editorWidget.border": "#2979FF",
        "settings.modifiedItemIndicator": "#2979FF",
        "settings.headerForeground": "#2979FF",
        "panelTitle.activeBorder": "#2979FF",
        "breadcrumb.activeSelectionForeground": "#2979FF",
        "menu.selectionForeground": "#2979FF",
        "menubar.selectionForeground": "#2979FF"
    },
    "materialTheme.autoApplyIcons": true,
    "files.autoSave": "afterDelay",
    "explorer.sortOrder": "type" ,
    "editor.formatOnSave": true,
    "editor.suggestSelection": "first",
    "editor.cursorStyle": "block",
    "editor.cursorBlinking": "smooth",
    "editor.acceptSuggestionOnEnter": "smart",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.minimap.enabled": false,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "yaml.schemas": {
        "https://raw.githubusercontent.com/fgeckeler/cap-demo/master/pipeline_config.schema.json": "*/pipeline_config.yml"
    },
    "yaml.completion": true,
    "yaml.schemaStore.enable": true,
    "yaml.validate": true,
    "editor.tokenColorCustomizations": {
            "comments": "#A0A4AB"
    },
    "http.proxyStrictSSL": false,
    "materialTheme.accent": "Orange",
    "go.useLanguageServer": true,
    "go.lintOnSave": "package",
    "go.vetOnSave": "package",
    "go.formatTool": "goreturns",
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.autocompleteUnimportedPackages": true,
    "window.zoomLevel": 0,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.jediEnabled": false,
    "debug.node.autoAttach": "off",
    "vscode-gardener-tools": {
        "vscode-light-theme": false,
        "landscapes": [
          {
            "name": "landscape-dev",
            "gardenName": "virtual-dev",
            "kubeconfigPath": "/kubeconfigpath/cluster-dev-virtual-garden/kubeconfig.yaml",
            "dashboardUrl": "https://dashboard.garden.dev.example.com",
            "projects": ["garden", "myproject"]
          },
          {
            "name": "landscape-canary",
            "kubeconfigPath": "/kubeconfigpath/cluster-canary-virtual-garden/kubeconfig.yaml",
            "dashboardUrl": "https://dashboard.garden.canary.example.com"
          },
        ]
      }

}
